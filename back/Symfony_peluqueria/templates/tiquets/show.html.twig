{% extends 'base.html.twig' %}

{% block title %}Tiquets{% endblock %}

{% block body %}
    <h1>Tiquets</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tiquet.id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ tiquet.fecha ? tiquet.fecha|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Precio_total</th>
                <td>{{ tiquet.precioTotal }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_tiquets_index') }}">back to list</a>

    <a href="{{ path('app_tiquets_edit', {'id': tiquet.id}) }}">edit</a>

    {{ include('tiquets/_delete_form.html.twig') }}
